# Backend configuration
# Set to 'duckdb' for local demo or 'snowflake' for production
BACKEND=duckdb

# DuckDB settings (for local demo)
DUCKDB_PATH=/data/warehouse.duckdb
DATA_BASE_PATH=/data

# S3/MinIO settings
S3_ENDPOINT=http://minio:9000
AWS_ACCESS_KEY_ID=minioadmin
AWS_SECRET_ACCESS_KEY=minioadmin
S3_BUCKET=fashion-retail-data-lake

# Gzip compression for files
GZIP_FILES=false

# Snowflake settings (fill in for production)
SNOWFLAKE_USER=your_snowflake_user
SNOWFLAKE_PASSWORD=your_snowflake_password
SNOWFLAKE_ACCOUNT=your_account.region
SNOWFLAKE_WAREHOUSE=FASHION_RETAIL_WH
SNOWFLAKE_DATABASE=FASHION_RETAIL_DB
SNOWFLAKE_SCHEMA=RETAIL_SCHEMA

# AWS S3 settings (for production)
AWS_S3_ACCESS_KEY_ID=your_aws_access_key
AWS_S3_SECRET_ACCESS_KEY=your_aws_secret_key
AWS_S3_REGION=us-east-1